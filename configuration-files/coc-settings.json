	"languageserver": {
		"ccls": {
			"enable": true,
			"command": "ccls",
			"filetypes": [
				"c",
				"cpp",
				"objc",
				"objcpp"
			],
			"rootPatterns": [
				".ccls",
				"compile_commands.json",
				".vim/",
				".git/",
				".hg/"
			],
			"initializationOptions": {
				"cache": {
					"directory": "/tmp/ccls"
				}
			}
		},
		"haskell": {
			"command": "haskell-language-server-wrapper",
			"args": [
				"--lsp"
			],
			"rootPatterns": [
				".stack.yaml",
				".hie-bios",
				"BUILD.bazel",
				"cabal.config",
				"package.yaml"
			],
			"filetypes": [
				"hs",
				"lhs",
				"haskell"
			],
			"initializationOptions": {
				"languageServerHaskell": {}
			},
			"trace.server": "verbose"
		},
		"matlab": {
			"command": "~/tmp",
			"filetypes": [
				"mma"
			]
		}
	},
	"python.linting.mypyEnabled": false,
	"python.analysis.typeshedPaths": [
		"/home/iernstig/miniconda3/lib/python3.9/site-packages/mypy/typeshed"
	],
	"coc.preferences.formatOnSaveFiletypes": [
		"json",
	],
	"snippets.snipmate.enable": true,
	"html.format.preserveNewLines": false,
	"html.format.indentInnerHtml": true,
	"html.format.wrapLineLength": 80,
	"html.format.enable": true,
	"html.filetypes": [
		"html"
	],
	"pydocstring.formatter": "google",
  "python.formatting.provider": "black",
  "python.analysis.typeCheckingMode": "off",
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/13.0.0/clangd_13.0.0/bin/clangd"
}
